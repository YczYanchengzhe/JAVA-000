<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="log4j2 : %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg %n" />
		</Console>
	</Appenders>
	<Loggers>
		<root level="info" includeLocation="true">
			<appender-ref ref="Console" />
		</root>
	</Loggers>
</configuration> 
